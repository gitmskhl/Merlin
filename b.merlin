
class Human {
    init(name, age, sex) {
        this.name = name;
        this.age = age;
        this.sex = sex;
    }

    show() {
        println(this.name, this.age, this.sex);
    }
}

class Man : Human {
    init(name, age) {
        super(name, age, "male");
    }
}

class Woman : Human {
    init(name, age) {
        super(name, age, "female");
    }
}

