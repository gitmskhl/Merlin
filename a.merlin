
class A {
    sqr(n) {
        return n * n;
    }
}

class B : A {
    cube(n) {
        return super.sqr(n) * n;
    }
}

class C : B {
    fourth(n) {
        return this.cube(n) * n;
    }

    fourth2(n) {
        return this.sqr(this.sqr(n));
    }
}

var c = C();

for (var i = 1; i < 5; i += 1) {
    println(i + " " + c.sqr(i) + " " + c.cube(i) + " " + c.fourth(i) + " " + c.fourth2(i));
}

